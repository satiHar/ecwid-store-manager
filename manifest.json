{
    "name": "Ecwid Store Manager",
    "description": "Ecwid Store Manager",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "48": "ecwidIcon.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "template.xml"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "permissions": [
        "scripting",
        "activeTab",
        "storage"
    ],
    "background": {}
}
